<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.okay">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TaskActivity" />
        <activity android:name=".activity.TeamActivity" />
        <activity android:name=".activity.UserInfoActivity" />
        <activity android:name=".activity.FileActivity" />
        <activity
            android:name=".activity.SetActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".activity.euc" />
        <activity
            android:name=".activity.Login"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".activity.Register"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".activity.Forget"
            android:windowSoftInputMode="adjustPan" />
    </application>

</manifest>
